{
  "name": "tna-sso-azure-template",
  "version": "1.0.0",
  "description": "This is a node application to render HTML templates for Microsoft Azure ADB2C",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "parcel src/index.html --open",
    "build": "parcel build src/index.html --out-dir prod",
    "lint": "./node_modules/.bin/eslint ./src/scripts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nationalarchives/tna-sso-azure-template.git"
  },
  "keywords": [
    "tna",
    "sso",
    "azure",
    "adb2c"
  ],
  "author": "Punal Chotrani, Ashley Burnett, Gwyn Jones",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nationalarchives/tna-sso-azure-template/issues"
  },
  "homepage": "https://github.com/nationalarchives/tna-sso-azure-template#readme",
  "dependencies": {
    "vue": "^2.6.6",
    "express": "^4.16.4",
    "express-ejs-layouts": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@vue/component-compiler-utils": "^2.5.2",
    "babel-preset-env": "^1.7.0",
    "ejs": "^2.6.1",
    "eslint": "^5.14.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "parcel-bundler": "^1.11.0",
    "prettier": "1.16.4",
    "sass": "^1.14.1"
  }
}
